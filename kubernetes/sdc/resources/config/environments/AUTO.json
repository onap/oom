    "json_class": "Chef::Environment",
    "chef_type": "environment",

    "default_attributes": {
        "CS_VIP": "sdc-cs.{{ .Values.nsPrefix }}-sdc",
        "BE_VIP": "sdc-be.{{ .Values.nsPrefix }}-sdc",
        "FE_VIP": "sdc-fe.{{ .Values.nsPrefix }}-sdc",
        "ES_VIP": "sdc-es.{{ .Values.nsPrefix }}-sdc",
        "interfaces": {
            "application": "eth0",
            "private": "eth1"
