{
  "service": {
    "name": "Health Check: MSB",
    "checks": [
      {
        "id": "msb-eag.onap-namespace",
        "name": "MSB eag Health Check",
        "http": "http://msb-eag.onap-namespace:80/iui/microservices/default.html",
        "method": "HEAD",
        "tls_skip_verify": true,
        "interval": "15s",
        "timeout": "1s"
      },
      {
        "id": "msb-iag.onap-namespace",
        "name": "MSB iag Health Check",
        "http": "http://msb-iag.onap-namespace:80/iui/microservices/default.html",
        "method": "HEAD",
        "tls_skip_verify": true,
        "interval": "15s",
        "timeout": "1s"
      },
      {
        "id": "msb-consul.onap-namespace",
        "name": "MSB consul Health Check",
        "tcp": "msb-consul.onap-namespace:8500",
        "interval": "15s",
        "timeout": "1s"
      },
      {
        "id": "msb-discovery.onap-namespace",
        "name": "MSB discovery Health Check",
        "tcp": "msb-discovery.onap-namespace:10081",
        "interval": "15s",
        "timeout": "1s"
      }
    ]
  }
}
