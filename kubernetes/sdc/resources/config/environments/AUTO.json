            "https_port": "9443"
        },
        "BE": {
            "http_port": "8080",
            "https_port": "8443"
        },
        "ONBOARDING_BE": {
            "http_port": "8081",
            "https_port": "8445",
            "catalog_notification_url": "%s://%s:%s/sdc2/rest/v1/catalog/notif/vsp/"
