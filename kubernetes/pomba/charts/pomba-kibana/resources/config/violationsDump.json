{
  "service-violations": {
    "mappings": {
      "default": {
        "properties": {
          "category": {
            "type": "text"
          },
          "message": {
            "type": "text"
          },
          "modelInvariantId": {
            "type": "text"
          },
          "modelVersionId": {
            "type": "text"
          },
          "serviceInstanceId": {
            "type": "text"
          },
          "severity": {
            "type": "text"
          },
          "validationId": {
            "type": "text"
          },
          "validationRule": {
            "type": "text"
          },
          "validationTimestamp": {
            "type": "date",
            "format": "MMM d y HH:m:s||dd-MM-yyyy HH:mm:ss||yyyy-MM-dd'T'HH:mm:ss.SSSZZ||MM/dd/yyyy||yyyyMMdd'T'HHmmssZ"
          },
          "violationId": {
            "type": "text"
          },
          "violationTimestamp": {
            "type": "date",
            "format": "MMM d y HH:m:s||dd-MM-yyyy HH:mm:ss||yyyy-MM-dd'T'HH:mm:ss.SSSZZ||MM/dd/yyyy||yyyyMMdd'T'HHmmssZ"
          },
          "violationType": {
            "type": "text"
          }
        }
      }
    }
  }
}