{
  "service": {
    "name": "Health Check: SDNC Portal",
    "checks": [
      {
        "id": "sdnc-portal",
        "name": "SDNC Portal Health Check",
        "http": "http://sdnc-portal:8843/login",
        "method": "HEAD",
        "header": {
          "Cache-Control": ["no-cache"],
          "Content-Type": ["application/json"],
          "Accept": ["application/json"]
        },
        "tls_skip_verify": true,
        "interval": "15s",
        "timeout": "1s"
      }
    ]
  }
}
